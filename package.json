{
  "name": "call-logger",
  "private": true,
  "version": "0.5.6",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --mode development",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check --plugin-search-dir=. ./src/**/*.svelte ./src/**/*.ts",
    "lint:fix": "prettier --write --plugin-search-dir=. ./src/**/*.svelte ./src/**/*.ts",
    "deploy:dev": "npm run lint && npm run build:dev && firebase deploy --project dev",
    "deploy:prod": "npm run lint && npm run build:prod && firebase deploy --project prod",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-svelte-csf": "^2.0.7",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/svelte": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/vite-plugin-svelte": "^1.0.8",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-loader": "^8.2.5",
    "firebase-tools": "^11.10.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.55.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-fa": "^3.0.3",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.3"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "bulma": "^0.9.4",
    "firebase": "^9.10.0",
    "rxfire": "^6.0.3",
    "rxjs": "^7.5.7",
    "unique-names-generator": "^4.7.1"
  }
}
