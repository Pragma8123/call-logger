{
  "name": "call-logger",
  "private": true,
  "version": "0.3.3",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --mode development",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check --plugin-search-dir=. ./src/**/*.svelte ./src/**/*.ts",
    "lint:fix": "prettier --write --plugin-search-dir=. ./src/**/*.svelte ./src/**/*.ts",
    "deploy:dev": "npm run build:dev && firebase deploy --project dev",
    "deploy:prod": "npm run build:prod && firebase deploy --project prod"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "firebase-tools": "^11.4.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.2",
    "bulma": "^0.9.4",
    "firebase": "^9.9.1",
    "rxfire": "^6.0.3",
    "rxjs": "^7.5.6",
    "unique-names-generator": "^4.7.1"
  }
}
